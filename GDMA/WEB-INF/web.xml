<web-app>
    <servlet>
        <servlet-name>Download</servlet-name>
        <display-name>Download</display-name>
        <servlet-class>com.vodafone.gdma.servlets.Download</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Download</servlet-name>
        <url-pattern>/export.xml</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Download</servlet-name>
        <url-pattern>/export.csv</url-pattern>
    </servlet-mapping>    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <error-page>
        <error-code>404</error-code>
        <location>/error.jsp</location>
    </error-page>
</web-app>
